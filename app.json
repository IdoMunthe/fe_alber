{
  "expo": {
    "name": "fe",
    "slug": "fe",
    "scheme" : "fe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/alberlogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/alberlogo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/alberlogo.png",
      "bundler" : "metro"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
