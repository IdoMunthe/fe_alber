{
  "expo": {
    "name": "Alber",
    "slug": "Alber",
    "scheme": "alber",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/alberlogo1.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#31AD1D"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/alberlogo1.png",
        "backgroundColor": "#31AD1D"
      },
      "package": "com.idomunthe.fe",
      "permissions": ["INTERNET"]
    },
    "web": {
      "favicon": "./assets/images/alberlogo.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "eas": {
      }
    }
  }
}
